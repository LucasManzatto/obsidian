		{"type":"text","text":"Ease of Use","id":"1e4acb33dc2b9b86","x":512,"y":2280,"width":250,"height":60,"color":"4"},
		{"type":"text","text":"Abstraction of RDD","id":"b8efa50bde25bb13","x":960,"y":2340,"width":250,"height":60,"color":"4"},
		{"type":"text","text":"Set of transformations and actions as operations","id":"8f62db7d37960428","x":960,"y":2220,"width":250,"height":60,"color":"4"},
		{"type":"text","text":"Extensibility","id":"679cbbdb39b1b8c4","x":512,"y":2880,"width":250,"height":60,"color":"4"},
		{"type":"text","text":"Modularity","id":"b17ef01345d1b6f5","x":512,"y":2600,"width":250,"height":60,"color":"4"},
		{"type":"text","text":"Programming languages","id":"dc3138dee76e16a9","x":960,"y":2520,"width":250,"height":60,"color":"4"},
		{"type":"text","text":"Scala, Python, SQL and R","id":"3423cdba46a3608c","x":1422,"y":2520,"width":250,"height":60,"color":"4"},
		{"type":"text","text":"Unified processing engine for all workloads","id":"e970d80a8a494390","x":960,"y":2660,"width":250,"height":60,"color":"4"},
		{"type":"text","text":"Unlike Hadoop, Spark decouples storage and compute","id":"dce3214b86aa77af","x":960,"y":2870,"width":280,"height":81,"color":"4"},
		{"type":"text","text":"Can read and write from multiple sources","id":"ac40bf422d5fa6e3","x":1422,"y":2881,"width":250,"height":60,"color":"4"},
